{
  "name": "reportabilidad-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:backend": "cd backend && node -r dotenv/config src/index.js",
    "start:frontend": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test": "npm-run-all test:backend test:frontend",
    "test:backend": "npm test --prefix backend",
    "test:frontend": "npm test --prefix frontend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "browser-image-compression": "^2.0.2",
    "concurrently": "^9.2.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
